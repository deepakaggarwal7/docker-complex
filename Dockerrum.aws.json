{
    "AWSEBDockerrunVersion": "2",
    "containers":[
        {
            "name":"server",
            "image":"deepakaggarwal/multi-docker-server",
            "essential":"false",
            "hostname": "api",
            "memory": 128
        },
        {
            "name":"client",
            "image":"deepakaggarwal/multi-docker-client",
            "essential":"false",
            "memory": 128
        },
        {
            "name":"worker",
            "image":"deepakaggarwal/multi-docker-worker",
            "essential":"false",
            "memory": 128
        },
        {
            "name":"nginx",
            "image":"deepakaggarwal/multi-docker-nginx",
            "essential":"true",
            "memory": 128,
            "links":["server","client"],
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ]
        }
    ]
}